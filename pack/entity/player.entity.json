				"blink": "controller.animation.persona.blink",
				"tooting_goat_horn": "animation.humanoid.tooting_goat_horn"
			},
			"render_controllers": [
				{
					"controller.render.blank": "q.is_alive && !v.is_paperdoll && !v.is_first_person && !q.is_in_ui"
				},
				{
					"controller.render.player.first_person": "variable.is_first_person"
				},
				{
					"controller.render.player.third_person": "!variable.is_first_person && !variable.map_face_icon && !variable.chyves1"
				},
				{
					"controller.render.player.third_person_bow": "!variable.is_first_person && !variable.map_face_icon && variable.chyves1"
				},
				{
					"controller.render.player.map": "variable.map_face_icon"
				},
				{
					"controller.render.status_hud": "query.is_in_ui && !variable.is_paperdoll"
				},
				{
					"controller.render.directionhud": "variable.is_paperdoll && !query.is_in_ui"
				},
				{
					"controller.render.chyves_cape": "!variable.is_first_person && query.armor_texture_slot(1) != 5 && !variable.chyves1"
				},
				{
					"controller.render.chyves_cape_bow": "!variable.is_first_person && query.armor_texture_slot(1) != 5 && variable.chyves1"
				},
				{
					"controller.render.chyves_speed": "query.is_in_ui && !variable.is_paperdoll"
				},
				{
					"controller.render.chyves_x1": "query.is_in_ui && !variable.is_paperdoll"
				},
				{
					"controller.render.chyves_x2": "query.is_in_ui && !variable.is_paperdoll"
				},
				{
					"controller.render.chyves_y1": "query.is_in_ui && !variable.is_paperdoll"
				},
				{
					"controller.render.chyves_y2": "query.is_in_ui && !variable.is_paperdoll"
				},
				{
					"controller.render.chyves_z1": "query.is_in_ui && !variable.is_paperdoll"
				},
				{
					"controller.render.chyves_z2": "query.is_in_ui && !variable.is_paperdoll"
				},
				{
					"controller.render.armor_helmet": "query.has_armor_slot(0)"
				},
				{
					"controller.render.armor_chestplate": "query.has_armor_slot(1)"
				},
				{
					"controller.render.armor_leggings": "query.has_armor_slot(2)"
				},
				{
					"controller.render.armor_boots": "query.has_armor_slot(3)"
				},
				{
					"controller.render.chyves_target_distance": "variable.is_paperdoll && !query.is_in_ui"
				},
				{
					"controller.render.chyves_target_health": "variable.is_paperdoll && !query.is_in_ui"
				}
			],
			"enable_attachables": true
		}
